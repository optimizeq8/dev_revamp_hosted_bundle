{
  "expo": {
    "name": "optimize",
    "scheme": "optimize",
    "description": "This project is really great.",
    "slug": "optimize_app",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#751AFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.optimizekw.optimize"
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },

    "android": {
      "package": "com.optimizekw.optimizw"
    }
  }
}
