{
  "expo": {
    "name": "OptimizeApp",
    "scheme": "optimize",
    "description": "Digital Marketing made easy.",
    "slug": "optimize_app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.3.60",
    "orientation": "portrait",
    "icon": "./assets/icon_1024.png",
    "notification": {
      "icon": "./assets/icon_1024.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#751AFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.optimizeapp.optimizeapp",
      "buildNumber": "65",
      "supportsTablet": false
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ],
      "config": "metro.config.js"
    },
    "android": {
      "package": "com.optimizeapp.optimizeapp",
      "versionCode": 47,
      "googleServicesFile": "./google-services.json"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "optimize",
            "project": "optimizeapp",
            "authToken": "cea83a7cc6f54a4b8a67f3db6a1ad43951b1145659c444f8b0f3931ec6e914c1"
          }
        }
      ]
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}